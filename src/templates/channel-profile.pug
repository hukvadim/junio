									option Stream Videos
									option Videos
						.catalog__tags.mobile-hide
							a.catalog__tag(href="#") All
							a.catalog__tag(href="#") Stream Videos
							a.catalog__tag(href="#") Videos
						.field
							.field__wrap
								select.field__select
									option Popular Videos
